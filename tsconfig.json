{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "sourceMap": false,
    "declaration": true,
    "outDir": "dist/",
    "declarationDir": "./"
  },
  "exclude": [
    "node_modules"
  ]
}
